properties([
 parameters([
  
     string(name: 'TEEIPAddress', defaultValue: "", description: 'Specify the TEEIPAddress for Persist')
   
 
 ])
 ])



node() {

instance_json="\$(aws ec2 describe-instances --region "us-east2" --filter Name=tag:Palatform,Values=TPA Nmae=private-ip-address,Values=$TEEIPAddress)"

Instnce_Id="\$(echo \$instance_json | jq -r '.Reservations[0].Instances[0].InstanceID');

aws ec2 describe-tags --region "us-east2" --instance-ids $Instnce_Id


}
