properties([
 parameters([
  
     string(name: 'TEEIPAddress', defaultValue: "", description: 'Specify the TEEIPAddress for Persist')
   
 
 ])
 ])

node() {

sh '''

instance_json="\$(aws ec2 describe-instances --region "us-east-1" --filter  Name=private-ip-address,Values=$TEEIPAddress)"

'''

}

